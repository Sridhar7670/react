{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/search/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/all",
      "dest": "/index.js"
    },
    {
      "src": "/register",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/login",
      "dest": "/index.js",
      "methods": ["POST"]
    }
  ]
}